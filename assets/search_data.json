{% capture json %}
[
  {% for doc in site.documents %}
    {
      "title"    : "{{ doc.title | escape }}",
      "category" : "{{ doc.categories[0] | xml_escape }}",
      "url"      : "{{ site.url }}{{ doc.url }}",
    } {% unless forloop.last %},{% endunless %}
  {% endfor %}
]
{% endcapture %}

{{ json | lstrip }}
